CXX=@CXX@

CPPFLAGS=@CPPFLAGS@ @BOOST_CPPFLAGS@ -DBOOST_LOG_DYN_LINK=1
CXXFLAGS=@CXXFLAGS@
LDFLAGS=@LDFLAGS@ @BOOST_LDFLAGS@
LIBS=@LIBS@ @BOOST_ASIO_LIB@ @BOOST_LOCALE_LIB@ @BOOST_REGEX_LIB@ @BOOST_PROGRAM_OPTIONS_LIB@ \
	@BOOST_LOG_LIB@

O_FILES=main.o

all: netmush
	@echo Done.

netmush: $(O_FILES)
	$(CXX) -o netmush $(CXXFLAGS) $(LDFLAGS) $(O_FILES) $(LIBS)


